{
  "manifest": {
    "apis": {
      "custom": {
        "endpoint": {
          "sourceDir": "lambda/custom",
          "uri": "ask-custom-MyFeedbackBot_Skill-v1-default"
        }
      }
    },
    "manifestVersion": "1.0",
    "permissions": [{
        "name": "alexa::profile:name:read"
      },
      {
        "name": "alexa::profile:email:read"
      },
      {
        "name": "alexa::profile:mobile_number:read"
      }
    ],
    "publishingInformation": {
      "locales": {
        "en-US": {
          "name": "My Feedback Bot",
          "summary": "I will help you to give feedback regarding your smart home devices. Reporting errors and requesting new functionality will become easy and intuitive.",
          "description": "Sample Full Description",
          "examplePhrases": [
            "Alexa open my feedback bot",
            "Alexa tell my feedback bot that I want to give feedback",
            "Alexa tell my feedback bot that I want to report an error"
          ],
          "keywords": []
        }
      },
      "isAvailableWorldwide": true,
      "testingInstructions": "Sample Testing Instructions.",
      "category": "KNOWLEDGE_AND_TRIVIA",
      "distributionCountries": []
    }
  }
}
